{:source-paths ["src"]
 :dependencies [[reagent "0.10.0"] [cider/cider-nrepl "0.22.3"]]
 :nrepl {:port 4444}
 :builds {:app {:target :browser
                :output-dir "public/js"
                :asset-path "/js"
                :main app.main/main!
                :devtools {:after-load app.main/reload!
                           :repl-init-ns app.main}
                :modules {:main {:init-fn app.main/main!}}}}}
